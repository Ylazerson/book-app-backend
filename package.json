{
    "name"        : "book-app-api",
    "version"     : "1.0.0",
    "description" : "API for the Book App",
    "main"        : "backend.js",
    "scripts"     : {
        "predevstart"   : "babel-node src/side_tools/startMessage.js",
        "devstart"      : "npm-run-all --parallel start-server lint:watch",
        "start-server"  : "babel-node src/main.js",
        "security-check": "nsp check",
        "localtunnel"   : "lt --port 8000 --subdomain lazmon",
        "share"         : "npm-run-all --parallel start-server localtunnel",
        "lint"          : "esw src --color",
        "lint:watch"    : "npm run lint -- --watch",
        "test-c1"       : "newman run postman_collections/bookIdController.postman_collection.json",
        "test-c2"       : "newman run postman_collections/bookRootController.postman_collection.json",
        "test-c3"       : "newman run postman_collections/courseIdController.postman_collection.json",
        "test-c4"       : "newman run postman_collections/courseRootController.postman_collection.json",
        "test-c5"       : "newman run postman_collections/authorRootController.postman_collection.json",
        "test"          : "npm-run-all test-c1 test-c2 test-c3 test-c4 test-c5",
        "clean-dist"    : "rimraf ./dist && mkdir dist",
        "prebuild"      : "npm-run-all clean-dist",
        "build"         : "babel-node build_tools/build.js",
        "start"         : "node dist/backend.js"
    },
    "author"      : "Yisroel Lazerson",
    "license"     : "ISC",
    "dependencies": {
        "babel-cli"           : "6.16.0",
        "babel-core"          : "6.17.0",
        "babel-loader"        : "6.2.5",
        "babel-preset-latest" : "6.16.0",
        "babel-register"      : "6.16.3",
        "body-parser"         : "^1.17.2",
        "chalk"               : "1.1.3",
        "compression"         : "1.6.2",
        "cors"                : "^2.8.3",
        "cross-env"           : "3.1.3",
        "dotenv"              : "^4.0.0",
        "eslint"              : "^4.2.0",
        "eslint-plugin-import": "^2.0.1",
        "eslint-watch"        : "^3.1.2",
        "express"             : "^4.13.3",
        "localtunnel"         : "1.8.1",
        "mssql"               : "^4.0.4",
        "newman"              : "^3.7.4",
        "npm-run-all"         : "3.1.1",
        "nsp"                 : "2.6.2",
        "rimraf"              : "^2.6.1",
        "source-map-support"  : "^0.2.10",
        "webpack"             : "^1.7.3"
    },
    "engines"     : {
        "node": "5.6.0"
    },
    "repository"  : {
        "type": "git",
        "url" : "https://github.com/Ylazerson/book-app-backend"
    }
}
